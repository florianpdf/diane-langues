<!-- Team -->
<section class="page-section" id="team">
	<div class="container">
		<div class="row">
			<div class="col-lg-12 text-center">
				<h2 class="section-heading text-uppercase">Domaines</h2>
				<h3 class="section-subheading text-muted">Voici les domaines dans lesquels je peux vous aider</h3>
			</div>
		</div>
		<div class="row">
			{% for domain in domains %}
				<div class="col-sm-4">
					<div class="team-member">
						<img class="mx-auto rounded-circle" src="{{ vich_uploader_asset(domain, 'imageFile') }}" alt="">
						<h4>{{ domain.type }}</h4>
						<p class="text-muted">{{ domain.description }}</p>
					</div>
				</div>
			{% endfor %}
		</div>
		<div class="row">
			<div class="col-lg-8 mx-auto text-center">
				<p class="large text-muted">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Aut eaque, laboriosam veritatis, quos non quis ad perspiciatis, totam corporis ea, alias ut unde.</p>
			</div>
		</div>
	</div>
</section>
