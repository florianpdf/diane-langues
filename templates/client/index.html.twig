{% extends "base.html.twig" %}

{% block body %}

{% trans_default_domain 'app' %}

	{% include 'client/_navbar.html.twig' %}

	<!-- Header -->
	<header class="masthead">
		<div class="container">
			<div class="intro-text">
				<div class="intro-lead-in">Welcome To</div>
				<div class="intro-heading text-uppercase">Diane Langues</div>
				{# <a class="btn btn-primary btn-xl text-uppercase js-scroll-trigger" href="#services">Tell Me More</a> #}
			</div>
		</div>
	</header>

	{% include 'client/_services.html.twig' %}

	{% include 'client/_domains.html.twig' %}

	{% include 'client/_portfolio.html.twig' %}

	{% include 'client/_about.html.twig' %}

	{% include 'client/_references.html.twig' %}

	{% include 'client/_contact.html.twig' %}

	<!-- Footer -->
	<footer class="footer">
		<div class="container">
			<div class="row align-items-center">
				<div class="col-md-4">
					<span class="copyright">Copyright &copy; Diane Langues 2020</span>
				</div>
				<div class="col-md-4">
					<ul class="list-inline social-buttons">
						{% if contact.twitterLink %}
						<li class="list-inline-item">
							<a href="{{ contact.twitterLink }}">
								<i class="fab fa-twitter"></i>
							</a>
						</li>
						{% endif %}
						{% if contact.facebookLink %}
						<li class="list-inline-item">
							<a href="{{ contact.facebookLink }}">
								<i class="fab fa-facebook-f"></i>
							</a>
						</li>
						{% endif %}						
						{% if contact.linkedinLink %}
						<li class="list-inline-item">
							<a href="{{ contact.linkedinLink }}">
								<i class="fab fa-linkedin-in"></i>
							</a>
						</li>
						{% endif %}						
					</ul>
				</div>
				<div class="col-md-4">
					<ul class="list-inline quicklinks">
						<li class="list-inline-item">
							Made By <a href="#">Florian</a> & <a href="#">Sichelle</a>
						</li>
					</ul>
				</div>
			</div>
		</div>
	</footer>

{% endblock body %}
